@article{kilukShortInventoryProblems2013,
  title = {The {{Short Inventory}} of {{Problems}} -- {{Revised}} ({{SIP-R}}): {{Psychometric}} Properties within a Large, Diverse Sample of Substance Use Disorder Treatment Seekers},
  shorttitle = {The {{Short Inventory}} of {{Problems}} -- {{Revised}} ({{SIP-R}})},
  author = {Kiluk, Brian D. and Dreifuss, Jessica A. and Weiss, Roger D. and Morgenstern, Jon and Carroll, Kathleen M.},
  year = {2013},
