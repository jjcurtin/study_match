@article{snaithScaleAssessmentHedonic1995a,
  title = {A Scale for the Assessment of Hedonic Tone the {{Snaith-Hamilton Pleasure Scale}}},
  author = {Snaith, R. P. and Hamilton, M. and Morley, S. and Humayan, A. and Hargreaves, D. and Trigwell, P.},
  year = {1995},
