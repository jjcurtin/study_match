  pmid = {23965298},
  keywords = {Checklist,Child,Data Interpretation Statistical,diagnostic efficiency,evidence-based medicine,Humans,Mood Disorders,Parents,Psychiatric Status Rating Scales,Psychology Child,receiver operating characteristic analysis,ROC Curve,sensitivity and specificity},
