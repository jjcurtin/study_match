}

@article{hughesNaturalHistoryAttempts2014,
  title = {Natural {{History}} of {{Attempts}} to {{Stop Smoking}}},
  author = {Hughes, John R. and Solomon, Laura J. and Naud, Shelly and Fingar, James R. and Helzer, John E. and Callas, Peter W.},
