  keywords = {Female,Humans,Hypertension,Logistic Models,Male,Middle Aged,Monitoring Physiologic,Obesity,Reproducibility of Results,Respiration Disorders,Risk Factors,Sex Factors,Sleep Apnea Syndromes,Snoring,Surveys and Questionnaires}
