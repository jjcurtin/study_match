@article{partosQuittingRollercoasterHow2013,
  title = {The {{Quitting Rollercoaster}}: {{How Recent Quitting History Affects Future Cessation Outcomes}} ({{Data From}} the {{International Tobacco Control}} 4-{{Country Cohort Study}})},
  shorttitle = {The {{Quitting Rollercoaster}}},
  author = {Partos, Timea R. and Borland, Ron and Yong, Hua-Hie and Hyland, Andrew and Cummings, K. Michael},
  year = {2013},
  month = sep,
  journal = {Nicotine \& Tobacco Research},
  volume = {15},
