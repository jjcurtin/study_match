  shorttitle = {Successful Classification of Cocaine Dependence Using Brain Imaging},
  author = {Mete, Mutlu and Sakoglu, Unal and Spence, Jeffrey S. and Devous, Michael D. and Harris, Thomas S. and Adinoff, Bryon},
