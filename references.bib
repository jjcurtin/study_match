}

@article{huangPerformanceMetricsComparative2021,
  title = {Performance {{Metrics}} for the {{Comparative Analysis}} of {{Clinical Risk Prediction Models Employing Machine Learning}}},
  author = {Huang, Chenxi and Li, Shu-Xia and Caraballo, C{\'e}sar and Masoudi, Frederick A. and Rumsfeld, John S. and Spertus, John A. and Normand, Sharon-Lise T. and Mortazavi, Bobak J. and Krumholz, Harlan M.},
